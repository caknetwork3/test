{
  "name": "cak-streamers-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Plataforma web para streamers de la agencia CAK",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "build:analyze": "vite build --mode analyze",
    "build:production": "npm run lint:check && npm run format:check && vite build",
    "serve": "vite preview",
    "clean": "rimraf dist node_modules/.vite",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "vue": "^3.5.17",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "terser": "^5.43.1",
    "vite": "^7.0.4"
  },
  "keywords": [
    "vue",
    "streamers",
    "gaming",
    "twitch",
    "platform",
    "cak"
  ],
  "author": "CAK Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cak-streamers/platform.git"
  },
  "bugs": {
    "url": "https://github.com/cak-streamers/platform/issues"
  },
  "homepage": "https://cak-streamers.com"
}
